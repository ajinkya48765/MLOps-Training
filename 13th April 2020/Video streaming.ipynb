    "import cv2\n",
    "cap = cv2.VideoCapture(0)\n",
    "while True :\n",
    "    status, photo = cap.read()\n",
    "    cv2.imshow('Video Streaming by Jinkxxx', photo)\n",
    "    if cv2.waitKey(10) == 13:\n",
    "        break\n",
    "cv2.destroyAllWindows()\n",
    "cap.release()"
   ]
